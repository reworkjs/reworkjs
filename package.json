    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prebump": "git flow release start $(standard-version --dry-run --release-as $RELEASE_TYPE | grep -E \"^âœ” bumping version in package.json from .+ to .+\" | grep -Eo \"[^ ]+$\") > /dev/null",
      "posttag": "git flow release finish -n $(cat package.json | json version)"
    }
  },
