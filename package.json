{
  "name": "@reworkjs/core",
  "version": "0.35.4",
  "description": "A react framework that works out of the box",
  "homepage": "https://www.reworkjs.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:reworkjs/reworkjs.git"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "author": "Zoé Cox <zoe@ephys.dev>",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./html": "./lib/_exports_/html.js",
    "./argv": {
      "node": "./lib/framework/common/app-argv/node.js",
      "webpack": "./lib/framework/common/app-argv/webpack.js"
    },
    "./babel-preset": "./lib/internals/babel/local-babel-preset.js",
    "./debug": "./lib/framework/common/debug.js",
    "./i18n": {
      "webpack": "./lib/_exports_/i18n.js"
    },
    "./router": {
      "webpack": "./lib/framework/common/router-api.js"
    },
    "./service-worker": {
      "webpack": "./lib/framework/common/service-worker-updater.js"
    },
    "./ssr": {
      "browser": "./lib/framework/common/ssr-apis/browser.js",
      "node": "./lib/framework/common/ssr-apis/node.js"
    },
    "./logger": {
      "node": "./lib/shared/logger/node.js",
      "default": "./lib/shared/logger/browser.js"
    },
    "./project-metadata": {
      "node": "./lib/shared/project-metadata/node.js",
      "webpack": "./lib/shared/project-metadata/webpack.js"
    },
    "./_internal_/react": "./lib/_exports_/react-runtime-hack.js",
    "./_internal_/react/jsx-runtime": "./lib/_exports_/jsx-runtime-hack.js",
    "./_internal_/react/jsx-dev-runtime": "./lib/_exports_/jsx-runtime-hack.js",
    "./_internal_/framework-metadata": {
      "node": "./lib/shared/framework-metadata/node.js",
      "webpack": "./lib/shared/framework-metadata/webpack.js"
    },
    "./_internal_/framework-config": {
      "node": "./lib/shared/framework-config/node.js",
      "browser": "./lib/shared/framework-config/webpack-client.js",
      "webpack": "./lib/shared/framework-config/webpack-server.js"
    },
    "./_internal_/render-html": {
      "node": "./lib/shared/render-html/node.js",
      "webpack": "./lib/shared/render-html/webpack.cjs"
    },
    "./_internal_/translations": {
      "node": "./lib/shared/translations/node.js",
      "webpack": "./lib/shared/translations/webpack.cjs"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "npm run lint:eslint && tsc src",
    "lint:eslint": "eslint src --report-unused-disable-directives",
    "lint:staged": "lint-staged",
    "build": "npm run build:clean && npm run build:babel",
    "build:babel": "concurrently npm:_build:babel:*",
    "_build:babel:js": "babel src --out-dir lib --extensions \".js,.ts,.tsx,.mts,.cts\" --out-file-extension \".js\" --ignore \"**/*.mts\" --ignore \"**/*.cts\" --ignore \"src/gatsby-theme-docz/**/*\" --copy-files --no-copy-ignored --quiet",
    "_build:babel:cjs": "babel src --out-dir lib --extensions \".cjs,.cts\" --out-file-extension \".cjs\" --ignore \"src/gatsby-theme-docz/**/*\" --quiet",
    "_build:babel:mjs": "babel src --out-dir lib --extensions \".mjs,.mts\" --out-file-extension \".mjs\" --ignore \"src/gatsby-theme-docz/**/*\" --quiet",
    "build:watch": "npm run build && npm run build:babel:watch",
    "build:babel:watch": "concurrently \"npm:_build:babel:js -- --watch --skip-initial-build\" \"npm:_build:babel:cjs -- --watch --skip-initial-build\" \"npm:_build:babel:mjs -- --watch --skip-initial-build\"",
    "build:clean": "del lib",
    "prepublishOnly": "npm run build",
    "release": "standard-version --release-as $RELEASE_TYPE",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:publish": "rm -rf es && rm -rf lib && npm run docz:build && cp CNAME .docz/dist/CNAME && gh-pages -d .docz/dist --message \"[skip ci] Publish docz\"",
    "test": "node ./tests/check-node-exports.js"
  },
  "bin": {
    "rjs": "./bin/framework.bin.js"
  },
  "standard-version": {
    "scripts": {
      "prebump": "git flow release start $(standard-version --dry-run --release-as $RELEASE_TYPE | grep -E \"^✔ bumping version in package.json from .+ to .+\" | grep -Eo \"[^ ]+$\") > /dev/null",
      "posttag": "git flow release finish -n $(cat package.json | json version)"
    }
  },
  "optionalDependencies": {
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
